{
  "scope": ".",
  "short_name": "React App",
  "name": "Create React App Sample",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64",
      "type": "image/x-icon"
    },
    {
      "src": "logo192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "logo512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "id": "/",
  "start_url": ".",
  "display": "browser",
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "minimal-ui",
    "browser"
  ],
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "screenshots": [
    {
      "src": "brand.png",
      "sizes": "1040x1040",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Wonder Widgets",
      "description": "Compress and compare images with different codecs right in your browser."
    },
    {
      "src": "brand.png",
      "sizes": "1040x1040",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Wonder Widgets Desktop",
      "description": "Compress and compare images with different codecs right in your browser."
    }
  ],
  "share_target": {
    "action": "/share",
    "enctype": "multipart/form-data",
    "method": "POST",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "media[]",
          "accept": ["audio/*", "image/*", "video/*"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+myapp",
      "url": "/?url=%s"
    }
  ]
}
